msgid "Log In"
msgstr "Kennaskañ"

#: src/user-list.vala:845
#, c-format
msgid "Login as %s"
msgstr "Kennaskañ evel %s"

#: src/user-list.vala:849
msgid "Retry"
msgstr "Klask en-dro"

#: src/user-list.vala:850
#, c-format
msgid "Retry as %s"
msgstr "Klask en-dro evel %s"

