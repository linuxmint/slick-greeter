#: ../src/user-list.vala:825
msgid "Log In"
msgstr "Prijava"

#: ../src/user-list.vala:826
#, c-format
msgid "Login as %s"
msgstr "Prijavi se kao %s"

#: ../src/user-list.vala:830
msgid "Retry"
msgstr "Pokušaj ponovno"

#: ../src/user-list.vala:831
#, c-format
